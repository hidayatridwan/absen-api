{
  "name": "ridwan-hidayat/absen-api",
  "description": "Rest API for Absen Apps",
  "type": "project",
  "authors": [
    {
      "name": "Ridwan Hidayat"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-pdo": "*",
    "firebase/php-jwt": "^6.4",
    "vlucas/phpdotenv": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.21"
  },
  "autoload": {
    "psr-4": {
      "RidwanHidayat\\Absen\\API\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RidwanHidayat\\Absen\\API\\": "test"
    }
  },
  "scripts": {
    "start": "php -S 127.0.0.1:83 -t public"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  }
}
